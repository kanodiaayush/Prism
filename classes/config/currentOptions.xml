<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE options SYSTEM "options.dtd">
<options>
<prism verbose="false" fairness="false" precomputation="true" compact="true" cuddMaxMem="204800" cuddEpsilon="1.0E-15" iterativeMethod="jacobi" overRelaxation="0.9" backwards="false" pseudo="false" termination="relative" maxIterations="10000" terminationEpsilon="1.0E-6" engine="hybrid" hybridNumLevels="-1" hybridMaxMemory="1024" hybridSORLevels="-1" hybridSORMaxMemory="1024"/>
<model autoParse="true" autoManual="true" parseDelay="1000">
<editor fontName="monospaced" fontStyle="0" fontSize="12" fontColourR="0" fontColourG="0" fontColourB="0" bgColourR="255" bgColourG="255" bgColourB="255">
<regEx description="Numeric" expression="\.|[0-9]" colourR="0" colourG="51" colourB="255" style="Plain"/>
<regEx description="Variable" expression="&#10;[A-Za-z_][A-Za-z0-9_]*|[A-Za-z_][A-Za-z0-9_]*|&#10;[A-Za-z_][A-Za-z0-9_]&#10;|&#10;[A-Za-z_][A-Za-z0-9_]" colourR="255" colourG="0" colourB="0" style="Plain"/>
<regEx description="Prism Keyword" expression="(\b|\p{Punct})(probabilistic|false|nondeterministic|stochastic|dtmc|mdp|ctmc|bool|int|double|const|formula|module|endmodule|true|false|global|init|prob|rate|min|max|system|endsystem|costs|endcosts|rewards|endrewards)(\b|\p{Punct})" colourR="0" colourG="0" colourB="0" style="Bold"/>
<regEx description="Single Line Comment" expression="//.*" colourR="0" colourG="99" colourB="0" style="Italic"/>
</editor>
<editor fontName="monospaced" fontStyle="0" fontSize="12" fontColourR="72" fontColourG="61" fontColourB="139" bgColourR="255" bgColourG="250" bgColourB="240">
<regEx description="Comment" expression="%\.*" colourR="0" colourG="153" colourB="0" style="Italic"/>
</editor>
</model>
<properties fontName="Monospaced" fontStyle="0" fontSize="12" selectionColourR="202" selectionColourG="225" selectionColourB="255" warningColourR="255" warningColourG="175" warningColourB="175" additionStrategy="warn" clearListOnLoad="true"/>
<simulator engine="prism" simultaneous="true" apmcStrategy="0" newPathAskDefault="true" renderAllValues="false"/>
<log fontName="monospaced" fontStyle="0" fontSize="12" selectionColourR="0" selectionColourG="0" selectionColourB="0" bgColourR="255" bgColourG="255" bgColourB="255" bufferLength="10000"/>
</options>
